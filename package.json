{
  "name": "piarch-a-verification",
  "version": "0.1.0",
  "description": "jwt verification service for piarch-a",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/ts-node server.ts",
    "start:docker": "docker-compose up --build -d",
    "stop:docker": "docker-compose down",
    "test:unit": "jest --config jest.config.js --coverage --forceExit --runInBand",
    "test:integration": "jest --config jest.integration.js --coverage --forceExit --runInBand",
    "prettier": "prettier -c .",
    "prettier:fix": "yarn prettier --write"
  },
  "dependencies": {
    "axios": "0.26.1",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "2.29.4",
    "mongoose": "6.2.10",
    "swagger-ui-express": "4.3.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "axios-mock-adapter": "1.20.0",
    "faker": "5.5.3",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4"
  }
}
